#include <iostream> 
#include <string> 
using namespace std; 
int main() 
{ 
  int a, b, c; 
  cout << "Enter variable values \n"; 
  cin >> a >> b >> c; 
  if (a*a + b*b == c*c) 
    cout << "The triangle is rectangular" ; 
  else 
    if (a*a == b*b + c*c) 
      cout << "The triangle is rectangular" ; 
    else 
      if (a*a + c*c == b*b) 
        cout << "The triangle is rectangular" ; 
      else 
        cout << "The triangle is not rectangular" ; 
}
