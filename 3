#include <iostream> 
#include <string> 
using namespace std; 
int main() 
{ 
  int A; 
  cout << "Enter A"; 
  cin >> A; 
  int b = A / 100; 
  int c = A % 2; 
  if (b < 1 and c == 0) 
    cout << "the number is two digit and positive" ; 
  else 
    cout << "the number isn't two digit and positive"; 
}
